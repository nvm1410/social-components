  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "eslint": "^7.32.0",
    "microbundle-crl": "^0.13.11"
  },
  "files": [
    "dist"
  ]
