  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/lib/index.js",
  "engines": {
    "node": ">=10"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "scripts": {
    "start": "react-scripts start  --openssl-legacy-provider",
