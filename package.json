  "dependencies": {
    "@babel/preset-react": "^7.18.6",
    "@babel/runtime": "^7.20.13",
    "@magic-ext/connect": "^4.0.0",
    "@orbisclub/orbis-sdk": "0.4.70",
    "@react-oauth/google": "^0.11.0",
